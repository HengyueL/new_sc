{
    "seed": 42,

    "log_folder": {
        "save_root": "SC-Train-1"
    },

    "wandb":{
        "init": false,
        "project": "SC_training"
    },

    "dataset":{
        "name": "cifar10",
        "dataset_path": "C:\\Users\\Liang\\dataset\\Cifar10\\Clean",
        "batch_size": 128,
        "num_workers": 8,
        "num_classes": 10
    },

    "model": {
        "name": "resnet",
        "standardized_fc": true
    },

    "train":{

        "total_epochs": 300,
        "print_interval": 500,

        "loss": {
            "name": "CE",
            "reduction": "mean"
        },

        "optimizer": {
            "name": "AdamW",
            "lr": 1e-3,
            "weight_decay": 2e-5
        },

        "scheduler": {
            "name": "CosineAnealing",
            "mode": null,
            "patience": null
        }
    },

    "val":{
        "loss": {
            "name": "CE",
            "reduction": "mean"
        }
    }
}